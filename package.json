{
	"name": "vue-todolist",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"preinstall": "npm run install:clean && npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
		"postinstall": "npm run lint",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"prettier:fix": "prettier --write \"./**/*.{vue,js,json}\"",
		"eslint:fix": "vue-cli-service lint \"**/*.{json,js,vue}\" --fix --ignore-path .eslintignore",
		"stylelint:fix": "stylelint \"**/*.{css,scss,sass,vue}\" --fix --ignore-path .stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"deploy": "node gh-pages-deploy.js"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.4.0",
		"@fortawesome/free-solid-svg-icons": "^6.4.0",
		"@fortawesome/vue-fontawesome": "^2.0.10",
		"chalk": "^4.1.2",
		"core-js": "^3.29.0",
		"execa": "^5.1.1",
		"node-emoji": "^1.11.0",
		"svg-symbol-sprite-loader": "^4.0.0",
		"vue": "^2.7.14",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.21.0",
		"@babel/eslint-parser": "^7.19.1",
		"@vue/cli-plugin-babel": "^4.5.15",
		"@vue/cli-plugin-eslint": "^4.5.15",
		"@vue/cli-plugin-vuex": "^4.5.15",
		"@vue/cli-service": "^4.5.15",
		"@vue/eslint-config-prettier": "^7.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.9.0",
		"eslint-webpack-plugin": "^2.7.0",
		"postcss": "^8.4.21",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.4",
		"sass": "^1.58.3",
		"sass-loader": "^10",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-property-sort-order-smacss": "^9.1.0",
		"stylelint-config-sass-guidelines": "^9.0.1",
		"stylelint-config-standard": "^26.0.0",
		"stylelint-scss": "^4.4.0",
		"stylelint-webpack-plugin": "^2.4.0",
		"style-resources-loader": "^1.5.0",
		"vue-cli-plugin-style-resources-loader": "~0.1.5",
		"vue-template-compiler": "^2.7.14"
	},
	"resolutions": {
		"ejs": ">=3.1.7",
		"glob-parent": ">=5.1.2",
		"json5": ">=2.2.2",
		"node-forge": ">=1.3.0",
		"nth-check": ">=2.0.1"
	},
	"volta": {
		"node": "14.19.3",
		"npm": "6.14.17"
	}
}
